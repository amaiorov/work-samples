/**
 * Typography Page
 */
{template .TypographyPage}
<div id="typography-page" class="page">
	<section class="masthead">
		<div class="loader"></div>
		<div class="grid">
			<h2>
				<span class="skewed">TYPE</span>
				<span>PRIMARY AND SECONDARY TYPOGRAPHY, ASSIGNED</span>
				<span>STYLES, MODIFIED CHARACTERS & MORE</span>
			</h2>
		</div>
	</section>

	<section data-id="primary-typeface">
		<div class="grid">
			<div class="section-heading c12">
				<h3>Primary Typeface</h3>
			</div>
			<div class="c12 framed-button-container">
				{call .FramedButton}
					{param text:'Get GZA Typeface' /}
					{param href:'http://fontdeck.com/font/gza/seminegra' /}
				{/call}
			</div>
			<div class="c8 push2">
				<h6>GZA SEMINEGRA<br>IS THE PRIMARY<br>TYPEFACE</h6>
				<p class="c12 sample">
					<span class="c2">GZA SEMINEGRA<br>ALL CAPS 36 PT</span>
					<span class="c10l">ABCDEFGHIJKLMNOPQRSTUVWXYZ 0123456789</span>
				</p>
				<p class="c12 sample">
					<span class="c2">GZA SEMINEGRA<br> 36 PT</span>
					<span class="c10l">abcdefghijklmnopqrstuvwxyz 0123456789</span>
				</p>
				<p class="c12 sample italic">
					<span class="c2">GZA SEMINEGRA<br> ITALIC</span>
					<span class="c10l">abcdefghijklmnopqrstuvwxyz 0123456789</span>
				</p>
				<p class="c12 sample italic">
					<span class="c2">GZA SEMINEGRA<br>ALL CAPS ITALIC</span>
					<span class="c10l">ABCDEFGHIJKLMNOPQRSTUVWXYZ 0123456789</span>
				</p>
			</div>
		</div>
	</section>

	<section data-id="modified-characters">
		<div class="grid">
			<div class="section-heading c12">
				<h3>Modified Characters</h3>
			</div>
			<div class="c12 framed-button-container">
				{call .FramedButton}
					{param text:'Download Custom Typeface' /}
					{param href:'downloads/animated-font.zip' /}
				{/call}
			</div>
			<img class="c10 push1" src="images/pages/typography/altered-characters.jpg" srcset="images/pages/typography/altered-characters@2x.jpg 2x">
		</div>
	</section>

	<section data-id="secondary-typeface">
		<div class="grid">
			<div class="section-heading c12">
				<h3>Secondary Typeface</h3>
			</div>
			<div class="c12 framed-button-container">
				{call .FramedButton}
					{param text:'Get Fugue Typeface' /}
					{param href:'http://www.radimpesko.com/fonts/fugue' /}
				{/call}
			</div>
			<div class="c8 push2">
				<h6>FUGUE IS THE<br>SECONDARY<br>TYPEFACE</h6>
				<p class="c12 sample headline">
					<span class="c2">FUGUE<br>HEADLINE</span>
					<span class="c10l">ABCDEFGHIJKLMNOPQRSTUVWXYZ 0123456789</span>
				</p>
				<p class="c12 sample">
					<span class="c2">FUGUE REGULAR<br>ALL CAPS</span>
					<span class="c10l">ABCDEFGHIJKLMNOPQRSTUVWXYZ 0123456789</span>
				</p>
				<p class="c12 sample">
					<span class="c2">FUGUE REGULAR<br></span>
					<span class="c10l">abcdefghijklmnopqrstuvwxyz 0123456789</span>
				</p>
				<p class="c12 sample italic">
					<span class="c2">FUGUE<br>ALL CAPS ITALIC</span>
					<span class="c10l">ABCDEFGHIJKLMNOPQRSTUVWXYZ 0123456789</span>
				</p>
				<p class="c12 sample mono">
					<span class="c2">FUGUE<br>MONO</span>
					<span class="c10l">ABCDEFGHIJKLMNOPQRSTUVWXYZ 0123456789</span>
				</p>
			</div>
		</div>
	</section>

	<section data-id="primary-styles">
		<div class="grid">
			<div class="section-heading on-black c12">
				<h3>Primary Styles</h3>
			</div>
			<div class="c3 push2">
				<p class="caption">
					<span>STYLE: RHIZOME 1.1. LOWERCASE</span>
					<span>BASED ON: GZA SEMINEGRA</span>
				</p>
				<p class="sample">
